{
  "name": "eru/sweet-alert",
  "description": "A simple PHP package to show Sweet Alerts with the Laravel Framework that supports",
  "keywords": [
    "laravel",
    "alert",
    "sweet",
    "notifier",
    "eru"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Erfan Ahmed Siam",
      "email": "erfan.siam98@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "~9.0",	
    "illuminate/session": "~9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.5",
    "friendsofphp/php-cs-fixer": "^3.00"
  },
  "autoload": {
    "psr-4": {
      "eru\\SweetAlert\\": "src/SweetAlert/"
    },
    "files": [
      "src/SweetAlert/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "eru\\SweetAlert\\SweetAlertServiceProvider"
      ],
      "aliases": {
        "Alert": "eru\\SweetAlert\\SweetAlert"
      }
    }
  }
}
